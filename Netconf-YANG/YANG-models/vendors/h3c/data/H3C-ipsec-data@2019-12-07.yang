module H3C-ipsec-data {

    namespace "http://www.h3c.com/netconf/data:1.0";

    prefix "ipsec-data";

    import comware-basetype {

         prefix "base";

    }

    import comware-extension {

         prefix "ext";

    }

    organization "NEW H3C Technologies Co., Ltd.";

    contact "Website: http://www.h3c.com";

    description "The module entities for ipsec data.";

    revision 2019-12-07 {

        description "Initial revision.";

    }

    container IPsec {

        config false;

        container TransformSets {

            description "IPsec transformSets information

                This table supports creating rows.";

            list TransformSet {

                description "Row name";

                key "Name";

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The name of the IPsec transform set, string, length: 1 to 63 characters";

                }

                leaf EncapMode {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Encapsulation mode 

                                                    Enumeration:

                                                    1-transport

                                                    2-tunnel";

                }

                leaf Proto {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "Security protocol

                                                    Enumeration:

                                                    1-AH

                                                    2-AH-ESP

                                                    3-ESP";

                }

                container ESPAuthAlgList {

                    description "The authentication algorithm for ESP";

                    leaf-list ESPAuthAlgItem {

                        type enumeration {

                            enum 6;

                            enum 7;

                            enum 8;

                            enum 9;

                            enum 10;

                            enum 130;

                        }

                    }

                }

                container ESPEncrAlgList {

                    description "The encryption algorithm fo ESP";

                    leaf-list ESPEncrAlgItem {

                        type enumeration {

                            enum 11;

                            enum 12;

                            enum 13;

                            enum 14;

                            enum 15;

                            enum 16;

                            enum 128;

                            enum 129;

                        }

                    }

                }

                container AHAuthAlgList {

                    description "The authentication algorithm number for AH";

                    leaf-list AHAuthAlgItem {

                        type enumeration {

                            enum 6;

                            enum 7;

                            enum 130;

                        }

                    }

                }

                leaf PFS {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..5";

                    }

                    description "Perfect Forward Secrecy, the Diffie-Hellman group ID is defined by IPsecCapabilities table";

                }

                leaf EsnEnable {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "EsnEnable  

                                                    Enumeration:

                                                    1-DISABLE

                                                    2-ESN

                                                    3-ESN and non-ESN";

                }

            }

        }

        container GlobalConf {

            description "IPsec global configuration information";

            leaf DecryptCheck {

                type base:Basetype_Boolean;

                description "ACL checking for de-encapsulated IPsec packets

                                        Boolean:

                                        true—Enable(default)

                                        false—Disable";

            }

            leaf LoggingPacket {

                type base:Basetype_Boolean;

                description "Logging for IPsec packets

                                        Boolean:

                                        true-Enable(default)

                                        false-Disable";

            }

            leaf DFBit {

                type enumeration {

                    enum 1;

                    enum 2;

                    enum 3;

                }

                description "Set the DF bit for outer IP headers of encapsulated IPsec packets on all interface

                                        Enumeration:

                                        1-clear

                                        2-copy(default)

                                        3-set";

            }

            leaf Redundancy {

                type base:Basetype_Boolean;

                description "IPsec redundancy function

                                        Boolean:

                                        true-Enable

                                        false-Disable(default)";

            }

            container AntiReplay {

                leaf State {

                    type base:Basetype_Boolean;

                    description "Anti-replay state

                                                    Boolean：

                                                    true-Enable(default)

                                                    false-Disable";

                }

                leaf CheckWindow {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                        enum 4;

                        enum 5;

                    }

                    description "numeration:

                                                    1-Window size is 64(default)

                                                    2-Window size is 128

                                                    3-Window size is 256

                                                    4-Window size is 512

                                                    5-Window size is 1024";

                }

            }

            container SADuration {

                leaf Seconds {

                    type base:Basetype_UnsignedInteger32 {

                        range "180..604800";

                    }

                    description "The time-based global lifetime of IPsec SA, unsigned integer, value range: 180 to 604800, in seconds, the default is 3600";

                }

                leaf KiloBytes {

                    type base:Basetype_UnsignedInteger32 {

                        range "2560..4294967295";

                    }

                    description "The traffic-based global lifetime of IPsec SA, unsigned integer, value range: 2560 to 4294967295, in kiloBytes, the default is 1843200";

                }

            }

            leaf SAIdleTime {

                type base:Basetype_UnsignedInteger32 {

                    range "60..86400";

                }

                description "SA idle timeout functionUnsigned integer, value range: 60 to 86400, in seconds";

            }

            leaf FlowRedirect {

                type base:Basetype_Boolean;

                description "IPsec FlowRedirect function

                                        Boolean:

                                        true-Enable

                                        false-Disable(default)";

            }

            leaf EstablishTunnelLimit {

                type base:Basetype_UnsignedInteger32;

                description "The Establish Tunnel Limit of IPsec , unsigned integer, value range: 1 to 4294967295, the default is 4294967295";

            }

            leaf Fragmentation {

                type enumeration {

                    enum 1;

                    enum 2;

                }

                description "Fragment MTU-exceeded packets

                                        Enumeration:

                                        1-Fragment packets before IPsec encapsulation (default)

                                        2-Fragment packets after IPsec encapsulation";

            }

            leaf LoggingNegotiation {

                type base:Basetype_Boolean;

                description "Logging for IPsec Negotiation

                                        Boolean:

                                        true-Enable(default)

                                        false-Disable";

            }

            container SASoftDuration {

                leaf Seconds {

                    type base:Basetype_UnsignedInteger32 {

                        range "20..201600";

                    }

                    description "The time-based global lifetime of IPsec SAUnsigned integer

                                                  value range: 20 to 201600,";

                }

                leaf KiloBytes {

                    type base:Basetype_UnsignedInteger32 {

                        range "1000..4294901760";

                    }

                    description "The traffic-based global lifetime of IPsec SA, Unsigned integer

                                                  value range: 1000 to 4294901760,";

                }

            }

        }

        container SAs {

            description "IPsec SAs";

            list SA {

                description "Row name";

                key "TunnelID SAIndex";

                leaf TunnelID {

                    type base:Basetype_UnsignedInteger32;

                    description "IPsec tunnel ID which the SA belongs to";

                }

                leaf SAIndex {

                    type base:Basetype_UnsignedInteger32;

                    description "SA index";

                }

                leaf Direction {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "SA direction

                                                    Enumeration:

                                                    1-Inbound

                                                    2-OutBound";

                }

                leaf Proto {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Security protocol

                                                    Enumeration:

                                                    1-AH

                                                    2-ESP";

                }

                leaf SPI {

                    type base:Basetype_UnsignedInteger32;

                    description "Security Parameter Index, unsigned integer, value range: 0 to 4294967295";

                }

                leaf SAMode {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "SA negotiation mode

                                                    Enumeration:

                                                    1-Manual

                                                    2-ISAKMP

                                                    3-Template";

                }

                leaf EncapMode {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Encapsulation mode

                                                    Enumeration:

                                                    1-transport

                                                    2-tunnel";

                }

                leaf PFS {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..5";

                    }

                    description "Perfect Forward Secrecy

                                                    The Diffie-Hellman group ID is defined by IPsecCapabilities table";

                }

                leaf AuthAlg {

                    type enumeration {

                        enum 0;

                        enum 6;

                        enum 7;

                    }

                    description "Authentication algorithm ID";

                }

                leaf EncrAlg {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..13";

                    }

                    description "Encryption algorithm ID";

                }

                container SADuration {

                    leaf Seconds {

                        type base:Basetype_UnsignedInteger32 {

                            range "180..604800";

                        }

                        description "The time-based global lifetime of IPsec SA, unsigned integer, value range: 180 to 604800, in seconds";

                    }

                    leaf KiloBytes {

                        type base:Basetype_UnsignedInteger32 {

                            range "2560..4294967295";

                        }

                        description "The traffic-based global lifetime of IPsec SA, unsigned integer, value range: 2560 to 4294967295, in kiloBytes";

                    }

                }

                container SARemainingDuration {

                    leaf Seconds {

                        type base:Basetype_UnsignedInteger32 {

                            range "180..604800";

                        }

                        description "Remaining IPsec SA lifetime in seconds, unsigned integer, value range: 180 to 604800, in seconds";

                    }

                    leaf KiloBytes {

                        type base:Basetype_UnsignedInteger32 {

                            range "2560..4294967295";

                        }

                        description "Remaining IPsec SA lifetime in kilobytes, Unsigned integer, value range: 2560 to 4294967295";

                    }

                }

                leaf RemainingIdleTime {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..86400";

                    }

                    description "Remaining IPsec SA idle time, Unsigned integer, value range: 0 to 86400";

                }

                leaf NatTraversal {

                    type base:Basetype_Boolean;

                    description "UDP encapsulation used for NAT traversal

                                                    Boolean:

                                                    true-Enable

                                                    false-Disable";

                }

                container AntiReplay {

                    leaf State {

                        type base:Basetype_Boolean;

                        description "Anti-replay state

                                                                Boolean:

                                                                true-Enable

                                                                false-Disable";

                    }

                    leaf CheckWindow {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                            enum 4;

                            enum 5;

                        }

                        description "Anti-replay window size

                                                         		Enumeration:

                                                                1-Window size is 64 

                                                                2-Window size is 128

                                                                3-Window size is 256

                                                                4-Window size is 512

                                                                5-Window size is 1024";

                    }

                }

                leaf PathMTU {

                    type base:Basetype_UnsignedInteger32;

                    description "Path MTU of the IPsec SA, unsigned integer";

                }

                leaf MaxRcvSN {

                    type base:Basetype_UnsignedInteger32;

                    description "Max sequence number in the received packets";

                }

                leaf MaxSendSN {

                    type base:Basetype_UnsignedInteger32;

                    description "Max sequence number in the sent packets";

                }

                leaf Status {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Stateful failover status of the IPsec SA

                                                   Enumeration:

                                                   1-active

                                                   2-standy";

                }

                leaf IfIndex {

                    type base:Basetype_NameAndIndexUnion;

                    description "Interface index";

                }

                leaf PolicyName {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Name of the used IPsec policy, string, length: 1 to 63 characters";

                }

                leaf PolicySeqNum {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65535";

                    }

                    description "Sequence number of the IPsec policy, unsigned integer, value range: 1 to 65535";

                }

            }

        }

        container Tunnels {

            description "IPsec Tunnels";

            list Tunnel {

                description "Row name";

                key "TunnelID";

                leaf TunnelID {

                    type base:Basetype_UnsignedInteger32;

                    description "IPsec tunnel ID";

                }

                leaf AddrType {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "IP protocol type

                                                	Enumeration:

                                                	1-Ipv4Address 

                                                	2-Ipv6Address";

                }

                leaf Status {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Stateful failover status of the IPsec SA

                                                   Enumeration:

                                                   1-active

                                                   2-standby";

                }

                leaf SAMode {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "SA negotiation mode

                                                    Enumeration:

                                                    1-Manual

                                                    2-ISAKMP

                                                    3-Template";

                }

                leaf PFS {

                    type base:Basetype_UnsignedInteger32;

                    description "Perfect Forward Secrecy: The Diffie-Hellman group ID is defined by IPsecCapabilities table";

                }

                leaf ACLNumber {

                    type base:Basetype_ACLNumberAdvance;

                    description "ACL Number, unsigned integer, value range: 3000 to 3999";

                }

                leaf LocalAddr {

                    type base:Basetype_IpAddress;

                    description "Source IP address of the IPsec tunnel";

                }

                leaf RemoteAddr {

                    type base:Basetype_IpAddress;

                    description "Destination IP address of the IPsec tunnel";

                }

                container SrcFlow {

                    leaf Proto {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..255";

                        }

                        description "Source protocol type";

                    }

                    leaf Addr {

                        type base:Basetype_IpAddress;

                        description "Source IP address";

                    }

                    leaf MaskLen {

                        type base:Basetype_UnsignedInteger32;

                        description "Source IP address mask length";

                    }

                    leaf Port {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..65535";

                        }

                        description "Source port number";

                    }

                }

                container DestFlow {

                    leaf Proto {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..255";

                        }

                        description "Destination  protocol type";

                    }

                    leaf Addr {

                        type base:Basetype_IpAddress;

                        description "Destination  IP address, Ipv4 address or Ipv6 address";

                    }

                    leaf MaskLen {

                        type base:Basetype_UnsignedInteger32;

                        description "Destination  IP address mask length";

                    }

                    leaf Port {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..65535";

                        }

                        description "Destination  port number";

                    }

                }

                leaf SecurityProto {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "IPsec SecurityProtocol

                                                  Enumeration:

                                                  1-AH

                                                  2-AH-ESP

                                                  3-ESP";

                }

                container SPI {

                    leaf ESPInbound {

                        type base:Basetype_UnsignedInteger32;

                        description "ESP inbound SPI";

                    }

                    leaf ESPOutbound {

                        type base:Basetype_UnsignedInteger32;

                        description "ESP outbound SPI";

                    }

                    leaf AHInbound {

                        type base:Basetype_UnsignedInteger32;

                        description "AH inbound SPI";

                    }

                    leaf AHOutbound {

                        type base:Basetype_UnsignedInteger32;

                        description "AH Outbound SPI";

                    }

                }

                leaf PolicyName {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The ISAKMP policy name, string, length: 1 to 63 characters";

                }

                leaf PolicySeqNum {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65535";

                    }

                    description "The sequence number for the IPsec policy, unsigned integer, value range: 1 to 65535";

                }

                leaf IfIndex {

                    type base:Basetype_NameAndIndexUnion;

                    description "Interface index";

                }

                leaf InsideVRF {

                    type base:Basetype_String {

                        length "1..31";

                    }

                    description "Inside VRF";

                }

            }

        }

        container IPv4PolicyTemplates {

            description "IPv4 policy templates

                This table supports creating rows.";

            list Template {

                description "Row name";

                key "Name SeqNum";

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The ISAKMP policy name, string, length: 1 to 63 characters";

                }

                leaf SeqNum {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65535";

                    }

                    description "The sequence number for the IPsec policy, unsigned integer, value range: 1 to 65535";

                }

                leaf Description {

                    type base:Basetype_Text {

                        length "0..80";

                    }

                    description "Description for an IPsec policy, string, length: 1 to 80 characters";

                }

                container ACL {

                    leaf Name {

                        type base:Basetype_ACLName;

                        description "ACL nameString, string, length: 1 to 63 characters";

                    }

                    leaf Number {

                        type base:Basetype_ACLNumberAdvance;

                        description "ACL numberUnsigned integer, value range: 3000 to 3999";

                    }

                    leaf Mode {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                        }

                        description "Protection modes:

                                                                Enumeration:

                                                                1-per-rule

                                                                2-per-host

                                                                3-aggregation";

                    }

                }

                container Remote {

                    leaf Addr {

                        type base:Basetype_Ipv4Address;

                        description "Remote IP address";

                    }

                    leaf HostName {

                        type base:Basetype_String {

                            length "1..253";

                        }

                        description "Remote host name";

                    }

                }

                leaf LocalAddr {

                    type base:Basetype_Ipv4Address;

                    description "Local IP address for the IPsec policy: Ipv4 address";

                }

                leaf IKEProfile {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Specify an IKE profile for the IPsec policyString, string, length: 1 to 63 characters.";

                }

                container TransformsetList {

                    description "IPsec transform set  nameString, length: 1 to 63 characters, Contains up to 6 Transformset in TransformsetList";

                    leaf-list Transformset {

                        type base:Basetype_String {

                            length "1..63";

                        }

                    }

                }

                leaf QosPreClassify {

                    type base:Basetype_Boolean;

                    description "QoS pre-classify feature, Boolean: true:Enable, false: Disable(default)";

                }

                container RRI {

                    leaf RRIEnable {

                        type base:Basetype_Boolean;

                        description "The IPsec reverse route inject (RRI) feature: Boolean: true, Enable, false, Disable(default)";

                    }

                    leaf Preference {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..255";

                        }

                        description "The preference of the static routes, unsigned integer, 1 to 255";

                    }

                    leaf Tag {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..4294967295";

                        }

                        description "The route tag for the static routes, 1 to 4294967295";

                    }

                }

                container SADuration {

                    leaf Seconds {

                        type base:Basetype_UnsignedInteger32 {

                            range "180..604800";

                        }

                        description "The time-based lifetime of IPsec SA: unsigned integer, 180 to 604800";

                    }

                    leaf KiloBytes {

                        type base:Basetype_UnsignedInteger32 {

                            range "2560..4294967295";

                        }

                        description "The traffic-based lifetime of IPsec SA, unsigned integer, 2560 to 4294967295";

                    }

                }

                leaf SAIdleTime {

                    type base:Basetype_UnsignedInteger32 {

                        range "60..86400";

                    }

                    description "SA idle timeout function, unsigned integer, 60 to 86400";

                }

                container Redundancy {

                    leaf InboundSyncInterval {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..1000";

                        }

                        description "Inbound synchronization interva";

                    }

                    leaf OutboundSyncInterval {

                        type base:Basetype_UnsignedInteger32 {

                            range "1000..100000";

                        }

                        description "Outbound synchronization interval";

                    }

                }

                leaf IKEv2Profile {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Specify an IKEv2 profile for the IPsec policy, string, length: 1 to 63 characters";

                }

                leaf TfcEnable {

                    type base:Basetype_Boolean;

                    description "QoS pre-classify feature

                                                 Boolean:

                                                 true-Enable

                                                 false-Disable(default)";

                }

            }

        }

        container IPv6PolicyTemplates {

            description "Displays IPv6 policy templates information

                This table supports creating rows.";

            list Template {

                description "Row name";

                key "Name SeqNum";

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The ISAKMP policy name, 1 to 63 characters";

                }

                leaf SeqNum {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65535";

                    }

                    description "The sequence number for the IPsec policy, 1 to 65535.";

                }

                leaf Description {

                    type base:Basetype_Text {

                        length "0..80";

                    }

                    description "Description for an IPsec policy, 1 to 80 characters";

                }

                container ACL {

                    leaf Name {

                        type base:Basetype_ACLName;

                        description "ACL name, 1 to 63 characters";

                    }

                    leaf Number {

                        type base:Basetype_ACLNumberAdvance;

                        description "ACL number, unsigned integer, 3000 to 3999";

                    }

                    leaf Mode {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                        }

                        description "Protection modes

                                                                Enumeration

                                                                1-per-rule

                                                                2-per-host

                                                                3-aggregation";

                    }

                }

                container Remote {

                    leaf Addr {

                        type base:Basetype_Ipv6Address;

                        description "Remote IP address";

                    }

                    leaf HostName {

                        type base:Basetype_String {

                            length "1..253";

                        }

                        description "Remote host name";

                    }

                }

                leaf LocalAddr {

                    type base:Basetype_Ipv6Address;

                    description "Local IP address for the IPsec policy, Ipv6 address";

                }

                leaf IKEProfile {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Specify an IKE profile for the IPsec policy, 1 to 63 characters";

                }

                container TransformsetList {

                    description "Reference an IPsec transform set for the IPsec policy";

                    leaf-list Transformset {

                        type base:Basetype_String {

                            length "1..63";

                        }

                    }

                }

                leaf QosPreClassify {

                    type base:Basetype_Boolean;

                    description "QoS pre-classify feature, Boolean, true: Enable, false: Disable(default)";

                }

                container RRI {

                    leaf RRIEnable {

                        type base:Basetype_Boolean;

                        description "The IPsec reverse route inject (RRI) feature, Boolean: true: Enable,false:Disable(default)";

                    }

                    leaf Preference {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..255";

                        }

                        description "The preference of the static routes, Unsigned integer, 1 to 255";

                    }

                    leaf Tag {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..4294967295";

                        }

                        description "The route tag for the static routes, 1 to 4294967295";

                    }

                }

                container SADuration {

                    leaf Seconds {

                        type base:Basetype_UnsignedInteger32 {

                            range "180..604800";

                        }

                        description "The time-based lifetime of IPsec SA, unsigned integer, 180 to 604800";

                    }

                    leaf KiloBytes {

                        type base:Basetype_UnsignedInteger32 {

                            range "2560..4294967295";

                        }

                        description "The traffic-based lifetime of IPsec SA, unsigned integer, 2560 to 4294967295";

                    }

                }

                leaf SAIdleTime {

                    type base:Basetype_UnsignedInteger32 {

                        range "60..86400";

                    }

                    description "SA idle timeout function, unsigned integer, 60 to 86400";

                }

                container Redundancy {

                    leaf InboundSyncInterval {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..1000";

                        }

                        description "Inbound synchronization interval";

                    }

                    leaf OutboundSyncInterval {

                        type base:Basetype_UnsignedInteger32 {

                            range "1000..100000";

                        }

                        description "Outbound synchronization interval";

                    }

                }

                leaf IKEv2Profile {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Specify an IKEv2 profile for the IPsec policy, string, length: 1 to 63 characters";

                }

                leaf TfcEnable {

                    type base:Basetype_Boolean;

                    description "QoS pre-classify feature

                                                 Boolean:

                                                 true-Enable

                                                 false-Disable(default)";

                }

            }

        }

        container IPv4ISAKMPPolicies {

            description "Displays IPv4 ISAKMP policies information

                This table supports creating rows.";

            list Policy {

                description "Row name";

                key "Name SeqNum";

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The ISAKMP policy name, 1 to 63 characters";

                }

                leaf SeqNum {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65535";

                    }

                    description "The sequence number for the IPsec policy, 1 to 65535";

                }

                leaf Description {

                    type base:Basetype_Text {

                        length "0..80";

                    }

                    description "Description for an IPsec policy, 1 to 80 characters";

                }

                container ACL {

                    leaf Name {

                        type base:Basetype_ACLName;

                        description "ACL name, 1 to 63 characters";

                    }

                    leaf Number {

                        type base:Basetype_ACLNumberAdvance;

                        description "ACL number, unsigned integer, 3000 to 3999";

                    }

                    leaf Mode {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                        }

                        description "Protection modes

                                                                Enumeration:

                                                                1-per-rule

                                                                2-per-host

                                                                3-aggregation";

                    }

                }

                container Remote {

                    leaf Addr {

                        type base:Basetype_Ipv4Address;

                        description "Remote IP address";

                    }

                    leaf HostName {

                        type base:Basetype_String {

                            length "1..253";

                        }

                        description "Remote host name";

                    }

                }

                leaf LocalAddr {

                    type base:Basetype_Ipv4Address;

                    description "Local IP address for the IPsec policy, Ipv4 address";

                }

