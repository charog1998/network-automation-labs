module H3C-sampler-data {

    namespace "http://www.h3c.com/netconf/data:1.0";

    prefix "sampler-data";

    import comware-basetype {

         prefix "base";

    }

    import comware-extension {

         prefix "ext";

    }

    organization "NEW H3C Technologies Co., Ltd.";

    contact "Website: http://www.h3c.com";

    description "The module entities for sampler data.";

    revision 2019-12-07 {

        description "Initial revision.";

    }

    container SAMPLER {

        config false;

        container Sampler {

            list SamplerEntry {

                key "SamplerName";

                leaf SamplerName {

                    type base:Basetype_String {

                        length "1..31";

                    }

                }

                leaf Mode {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..2";

                    }

                }

                leaf PacketInterval {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..max";

                    }

                }

            }

        }

        container SamplerSpecification {

            leaf SupportFixedMode {

                type base:Basetype_Boolean;

            }

            leaf SupportRandomMode {

                type base:Basetype_Boolean;

            }

            leaf MaxPacketInterval {

                type base:Basetype_UnsignedInteger32;

            }

            leaf MaxSamplerProfileNumber {

                type base:Basetype_UnsignedInteger32;

            }

        }

    }

}