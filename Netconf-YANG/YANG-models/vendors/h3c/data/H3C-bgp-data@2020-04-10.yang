                }

                leaf GroupName {

                    type base:Basetype_String {

                        length "1..47";

                    }

                    description "Group name (string, 1-47 characters)";

                }

                leaf RtPolicyImport {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Inbound routing policy (string, 1-63 characters).";

                }

                leaf RtPolicyExport {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Outbound routing policy (string, 1-63 characters).";

                }

                leaf AllowAsLoop {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..10";

                    }

                    description "Number of times the local AS number can appear in AS_PATH attribute of the received routes.";

                }

                leaf ReflectClient {

                    type enumeration {

                        enum 0;

                        enum 1;

                    }

                    description "Reflect client.

                        0-Disabled

                        1-Enabled";

                }

                leaf NexthopInvariable {

                    type enumeration {

                        enum 0;

                        enum 1;

                    }

                    description "Nexthop invariable.

                        0-Disabled

                        1-Enabled";

                }

                leaf NexthopLocal {

                    type enumeration {

                        enum 0;

                        enum 1;

                    }

