        container Redistributes {

            description "Redistribute route information

                This table supports creating rows.";

            list Redist {

                description "Redistribute route information table";

                key "Name VRF Family Protocol RedistName";

                leaf Name {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "0..31";

                        }

                    }

                    description "Process name (string, 0-31 characters)";

                }

                leaf VRF {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "0..31";

                        }

                    }

                    description "VRF name (string, 0-31 characters)";

                }

                leaf Family {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 5;

                        enum 6;

                    }

                    description "Address family

                        1—IPv4 Unicast

                        2—IPv4 Multicast

                        5—IPv6 Unicast

                        6—IPv6 Multicast";

                }

                leaf Protocol {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                        enum 4;

                        enum 5;

                        enum 6;

                        enum 8;

                    }

                    description "Protocol

                        1—Direct.

                        2—Static.

                        3—RIP

                        4—OSPF

                        5—ISIS

                        6—EIGRP

                        8—Guard";

                }

                leaf RedistName {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "0..31";

                        }

                    }

                    description "Protocol name. When the protocol is Direct or Static, the protocol name must be null. When the protocol is RIP, ISIS, or OSPF, the protocol name must be all-processes or an integer in the range of 1 to 65535.";

                }

                leaf AllowDirect {

                    type enumeration {

                        enum 0;

                        enum 1;

                    }

                    description "AllowDirect.

                        0-Disabled

                        1-Enabled";

                }

                leaf Med {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..4294967295";

                    }

                    description "The MED value of Redistributed routes.";

                }

                leaf RoutePolicy {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "RoutePolicy name(string, 1-63 characters).";

                }

            }

        }

        container SessionCounts {

            description "The count of sessions.";

            list SessionCount {

                description "Session count table";

                key "Name VRF State";

                leaf Name {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "0..31";

                        }

                    }

                    description "Process name (string, 0-31 characters)";

                }

                leaf VRF {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "0..31";

                        }

                    }

                    description "VRF name (string, 0-31 characters)";

                }

                leaf State {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..5";

                    }

