                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Source zone name";

                }

                leaf DestZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Destination zone name";

                }

                leaf ID {

                    type base:Basetype_UnsignedInteger32 {

                        range "min..65534";

                    }

                    description "Rule ID";

                }

                leaf Type {

                    type enumeration {

                        enum 1;

                    }

                    description "Type of the destination IPv4 address";

                }

                leaf Name {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Name of the data type";

                }

            }

        }

        container IPv4RuleServGrp {

            description "This table contains IPv4 object policy rule Service object group information.

                This table supports creating rows.";

            list RuleServGrp {

                key "SrcZone DestZone ID Name";

                leaf SrcZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Source zone name";

                }

                leaf DestZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Destination zone name";

                }

                leaf ID {

                    type base:Basetype_UnsignedInteger32 {

                        range "min..65534";

                    }

                    description "Rule ID";

                }

                leaf Name {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Name of the server object group";

                }

            }

        }

        container IPv4RuleApp {

            description "This table contains IPv4 object policy rule application information.

                This table supports creating rows.";

            list RuleApp {

                key "SrcZone DestZone ID Type Name";

                leaf SrcZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Source zone name";

                }

                leaf DestZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Destination zone name";

                }

                leaf ID {

                    type base:Basetype_UnsignedInteger32 {

                        range "min..65534";

                    }

                    description "Rule ID";

                }

                leaf Type {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Type of the application";

                }

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Name of the data type";

                }

            }

        }

