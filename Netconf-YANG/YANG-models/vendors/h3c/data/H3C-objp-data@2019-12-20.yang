                    description "Byte of matched packets";

                }

                leaf Profile {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "0..127";

                        }

                    }

                    description "Profile name";

                }

            }

        }

        container IPv6RuleSrcAddr {

            description "This table contains IPv6 object policy rule Source IPv6 address information.

                This table supports creating rows.";

            list RuleSrcAddr {

                key "SrcZone DestZone ID Type Name";

                leaf SrcZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Source zone name";

                }

                leaf DestZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Destination zone name";

                }

                leaf ID {

                    type base:Basetype_UnsignedInteger32 {

                        range "min..65534";

                    }

                    description "Rule ID";

                }

                leaf Type {

                    type enumeration {

                        enum 1;

                    }

                    description "Type of the source IPv6 address";

                }

