        container IPv6RuleApp {

            description "This table contains IPv6 object policy rule application information.

                This table supports creating rows.";

            list RuleApp {

                key "SrcZone DestZone ID Type Name";

                leaf SrcZone {

                    type base:Basetype_String {

                        length "0..31";

                    }

