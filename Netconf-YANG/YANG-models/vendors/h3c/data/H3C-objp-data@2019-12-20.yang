                        enum 3;

                    }

                    description "Action of object policy rule";

                }

                leaf SrcIPv6Group {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Source ipv6 address object group name";

                }

                leaf DestIPv6Group {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Destination ipv6 address object group name";

                }

                leaf ServGroup {

                    type base:Basetype_String {

                        length "0..31";

                    }

                    description "Service address object group name";

                }

                leaf AppGrp {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            length "1..63";

                        }

                    }

                    description "App group name";

                }

                leaf App {

                    type union {

