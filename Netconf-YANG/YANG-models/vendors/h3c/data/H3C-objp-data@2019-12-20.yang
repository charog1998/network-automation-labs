                        type base:Basetype_String {

                            length "0..31";

                        }

                    }

                    description "VRF name";

                }

                leaf TimeRange {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_String {

                            pattern "[^?	

]{1,32}";

                        }

                    }

                    description "Time range name";

                }

                leaf TimeRangeState {

                    type base:Basetype_Boolean;

                    description "State of time range";

                }

                leaf Enable {

                    type base:Basetype_Boolean;

                    description "Disable object policy rule";

                }

                leaf Log {

                    type base:Basetype_Boolean;

                    description "Logging of object policy rule";

                }

                leaf Counting {

                    type base:Basetype_Boolean;

                    description "Counting of object policy rule";

                }

                leaf Comment {

                    type union {

                        type base:Basetype_EmptyString;

                        type base:Basetype_Text {

                            length "0..127";

                        }

                    }

                    description "Comment of object policy rule";

                }

                leaf Count {

                    type base:Basetype_Counter64;

                    description "Count of matched packets";

                }

                leaf Byte {

