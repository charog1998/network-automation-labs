module H3C-ipsec-config {

    namespace "http://www.h3c.com/netconf/config:1.0";

    prefix "ipsec-config";

    import comware-basetype {

         prefix "base";

    }

    import comware-extension {

         prefix "ext";

    }

    organization "NEW H3C Technologies Co., Ltd.";

    contact "Website: http://www.h3c.com";

    description "The module entities for ipsec config.";

    revision 2019-12-07 {

        description "Initial revision.";

    }

    container IPsec {

        container TransformSets {

            description "IPsec transformSet information

                This table supports creating rows.";

            list TransformSet {

                description "Row name";

                key "Name";

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The name of the IPsec transform set, String, length: 1 to 63 characters";

                }

                leaf EncapMode {

                    type enumeration {

                        enum 1;

                        enum 2;

                    }

                    description "Encapsulation mode 

                                                    Enumeration:

                                                    1-transport

                                                    2-tunnel";

                }

                leaf Proto {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "Security protocol

                                                    Enumeration:

													1-Specifies the AH protocol.

													2-Specifies using the ESP protocol first and then using the AH protocol.

													3-Specifies the ESP protocol(default).";

                }

                container ESPAuthAlgList {

                    description "The authentication algorithm for ESP";

                    leaf-list ESPAuthAlgItem {

                        type enumeration {

                            enum 6;

                            enum 7;

                            enum 8;

                            enum 9;

                            enum 10;

                            enum 130;

                        }

                    }

                }

                container ESPEncrAlgList {

                    description "The encryption algorithm for ESP";

                    leaf-list ESPEncrAlgItem {

                        type enumeration {

                            enum 11;

                            enum 12;

                            enum 13;

                            enum 14;

                            enum 15;

                            enum 16;

                            enum 128;

                            enum 129;

                        }

                    }

                }

                container AHAuthAlgList {

                    description "The authentication algorithm number for AH";

                    leaf-list AHAuthAlgItem {

                        type enumeration {

                            enum 6;

                            enum 7;

                            enum 8;

                            enum 9;

                            enum 10;

                            enum 130;

                        }

                    }

                }

                leaf PFS {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..5";

                    }

                    description "Using PFS(Perfect Forward Secrecy), 

													the Diffie-Hellman group ID is defined by IPsecCapabilities table";

                }

                leaf EsnEnable {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "EsnEnable  

                                                    Enumeration:

                                                    1-DISABLE

                                                    2-ESN

                                                    3-ESN and non-ESN";

                }

            }

        }

        container GlobalConf {

            description "IPsec global configuration information";

            leaf DecryptCheck {

                type base:Basetype_Boolean;

                description "ACL checking for de-encapsulated IPsec packets, Boolean:

                                        true-Enable(default)

                                        false-Disable";

            }

            leaf LoggingPacket {

                type base:Basetype_Boolean;

                description "Logging for IPsec packets, Boolean:

                                        true-Enable

                                        false-Disable(default)";

            }

            leaf DFBit {

                type enumeration {

                    enum 1;

                    enum 2;

                    enum 3;

                }

                description "Set the DF bit for outer IP headers of encapsulated IPsec packets on all interface.

									Enumeration:

                                        1-clear

                                        2-copy(default)

                                        3-set";

            }

            leaf Redundancy {

                type base:Basetype_Boolean;

                description "IPsec redundancy function, Boolean:

                                        true: Enable

                                        false: Disable(default)";

            }

            container AntiReplay {

                leaf State {

                    type base:Basetype_Boolean;

                    description "Anti-replay state, Boolean:

													Boolean:

													true-Enable(default)

													false-Disable";

                }

                leaf CheckWindow {

                    type enumeration {

                        enum 1;

                        enum 2;

                        enum 3;

                        enum 4;

                        enum 5;

                    }

                    description "Anti-replay window size

													Enumeration:

													1-Window size is 64(default)

													2-Window size is 128

													3-Window size is 256

													4-Window size is 512

													5-Window size is 1024";

                }

            }

            container SADuration {

                leaf Seconds {

                    type base:Basetype_UnsignedInteger32 {

                        range "180..604800";

                    }

                    description "The time-based global lifetime of IPsec SAUnsigned integer, 

												  value range: 180 to 604800,

												  the default is 3600";

                }

                leaf KiloBytes {

                    type base:Basetype_UnsignedInteger32 {

                        range "2560..4294967295";

                    }

                    description "The traffic-based global lifetime of IPsec SA, Unsigned integer, 

												  value range: 2560 to 4294967295, 

												  the default is 1843200";

                }

            }

            leaf SAIdleTime {

                type base:Basetype_UnsignedInteger32 {

                    range "60..86400";

                }

                description "SA idle timeout function, Unsigned integer, value range: 60 to 86400";

            }

            leaf FlowRedirect {

                type base:Basetype_Boolean;

                description "IPsec FlowRedirect function, Boolean:

                                        true: Enable

                                        false: Disable(default)";

            }

            leaf EstablishTunnelLimit {

                type base:Basetype_UnsignedInteger32 {

                    range "1..4294967295";

                }

                description "Establish Tunnel Limit function, Unsigned integer, 

                                      value range: 1 to 4294967295,";

            }

            leaf Fragmentation {

                type enumeration {

                    enum 1;

                    enum 2;

                }

                description "Fragment MTU-exceeded packets.

                                    Enumeration:

                                        1-Fragment packets before IPsec encapsulation (default)

                                        2-Fragment packets after IPsec encapsulation";

            }

            leaf LoggingNegotiation {

                type base:Basetype_Boolean;

                description "Logging for IPsec Negotiation, Boolean:

                                        true-Enable

                                        false-Disable(default)";

            }

            container SASoftDuration {

                leaf Seconds {

                    type base:Basetype_UnsignedInteger32 {

                        range "20..201600";

                    }

                    description "The time-based global lifetime of IPsec SAUnsigned integer,

                                                  value range: 20 to 201600,";

                }

                leaf KiloBytes {

                    type base:Basetype_UnsignedInteger32 {

                        range "1000..4294901760";

                    }

                    description "The traffic-based global lifetime of IPsec SA, Unsigned integer,

                                                  value range: 1000 to 4294901760,";

                }

            }

        }

        container IPv4PolicyTemplates {

            description "IPv4 policy templates information

                This table supports creating rows.";

            list Template {

                description "Row name";

                key "Name SeqNum";

                leaf Name {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "The ISAKMP policy name, string, length: 1 to 63 characters";

                }

                leaf SeqNum {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65535";

                    }

                    description "The sequence number for the IPsec policy, unsigned integer, value range: 1 to 65535";

                }

                leaf Description {

                    type base:Basetype_Text {

                        length "0..80";

                    }

                    description "Description for an IPsec policyString, length: 1 to 80 characters";

                }

                container ACL {

                    leaf Name {

                        type base:Basetype_ACLName;

                        description "ACL nameString, string, length: 1 to 63 characters";

                    }

                    leaf Number {

                        type base:Basetype_ACLNumberAdvance;

                        description "ACL number, unsigned integer, value range: 3000 to 3999";

                    }

                    leaf Mode {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                        }

                        description "Protection modes:

                                                                Enumeration:

                                                                1: per-rule(default)

                                                                2: per-host

                                                                3: aggregation";

                    }

                }

                container Remote {

                    leaf Addr {

                        type base:Basetype_Ipv4Address;

                        description "Remote IP address";

                    }

                    leaf HostName {

                        type base:Basetype_String {

                            length "1..253";

                        }

                        description "Remote host name";

                    }

                }

                leaf LocalAddr {

                    type base:Basetype_Ipv4Address;

                    description "Local IP address for the IPsec policy";

                }

                leaf IKEProfile {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Specify an IKE profile for the IPsec policy, string, length: 1 to 63 characters";

                }

                container TransformsetList {

                    description "IPsec transform set name, string, length: 1 to 63 characters. Contains up to 6 Transformset in TransformsetList";

                    leaf-list Transformset {

                        type base:Basetype_String {

                            length "1..63";

                        }

                    }

                }

                leaf QosPreClassify {

                    type base:Basetype_Boolean;

                    description "QoS pre-classify feature

													Boolean:

													true-Enable

													false-Disable(default)";

                }

                container RRI {

                    leaf RRIEnable {

                        type base:Basetype_Boolean;

                        description "The IPsec reverse route inject (RRI) feature, Boolean:

														   true-Enable

														   false-Disable(default)";

                    }

                    leaf Preference {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..255";

                        }

                        description "The preference of the static routes, unsigned integer, value range: 1 to 255, default value is 60";

                    }

                    leaf Tag {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..4294967295";

                        }

                        description "The route tag for the static routes, Unsigned integer, value range: 1 to 4294967295, default value is 0";

                    }

                }

                container SADuration {

                    leaf Seconds {

                        type base:Basetype_UnsignedInteger32 {

                            range "180..604800";

                        }

                        description "The time-based lifetime of IPsec SA, unsigned integer, value range: 180 to 604800";

                    }

                    leaf KiloBytes {

                        type base:Basetype_UnsignedInteger32 {

                            range "2560..4294967295";

                        }

                        description "The traffic-based lifetime of IPsec SA, unsigned integer, value range: 2560 to 4294967295, in kiloBytes";

                    }

                }

                leaf SAIdleTime {

                    type base:Basetype_UnsignedInteger32 {

                        range "60..86400";

                    }

                    description "SA idle timeout functionUnsigned integer, value range: 60 to 86400, in seconds";

                }

                container Redundancy {

                    leaf InboundSyncInterval {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..1000";

                        }

                        description "Inbound synchronization interval";

                    }

                    leaf OutboundSyncInterval {

                        type base:Basetype_UnsignedInteger32 {

                            range "1000..100000";

                        }

                        description "Outbound synchronization interval";

                    }

                }

                leaf IKEv2Profile {

                    type base:Basetype_String {

                        length "1..63";

                    }

                    description "Specify an IKEv2 profile for the IPsec policy, string, length: 1 to 63 characters";

                }

                leaf TfcEnable {

                    type base:Basetype_Boolean;

                    description "QoS pre-classify feature

                                                 Boolean:

                                                 true-Enable

                                                 false-Disable(default)";

                }

            }

        }

