                            enum 17;

                        }

                        description "Source ProtocolType";

                    }

                    leaf SrcPortStart {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..65535";

                        }

                        description "Source PortStart";

                    }

                    leaf SrcPortEnd {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..65535";

                        }

                        description "Source PortEnd";

                    }

                    leaf DstIPAddr {

                        type base:Basetype_IpAddress;

                        description "Ip/Ipv6 Address";

                    }

                    leaf DstIPAddrLen {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..128";

                        }

                        description "Destination IPAddrLen";

                    }

                    leaf DstProtocolType {

                        type enumeration {

                            enum 6;

                            enum 17;

                        }

                        description "Destination ProtocolType";

                    }

                    leaf DstPortStart {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..65535";

                        }

                        description "Destination PortStart";

                    }

                    leaf DstPortEnd {

                        type base:Basetype_UnsignedInteger32 {

                            range "0..65535";

                        }

                        description "Destination PortEnd";

                    }

                }

                container Layer2 {

                    description "Layer2";

                    leaf SrcIfIndex {

                        type base:Basetype_NameAndIndexUnion;

                        description "SrcIPType";

                    }

                    leaf SrcVlan {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..4094";

                        }

                        description "SrcVlan";

                    }

                    leaf SrcMacAddr {

                        type base:Basetype_MacAddress;

                        description "SrcMacAddr";

                    }

                }

                leaf ObjGroupName {

                    type base:Basetype_String {

                        length "1..31";

                    }

                    description "ObjGroupName";

                }

                leaf HostName {

                    type base:Basetype_String {

                        length "1..253";

                    }

                    description "Host name";

                }

            }

        }

