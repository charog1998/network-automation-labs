module H3C-portal-config {

    namespace "http://www.h3c.com/netconf/config:1.0";

    prefix "portal-config";

    import comware-basetype {

         prefix "base";

    }

    import comware-extension {

         prefix "ext";

    }

    organization "NEW H3C Technologies Co., Ltd.";

    contact "Website: http://www.h3c.com";

    description "The module entities for portal config.";

    revision 2019-12-20 {

        description "Initial revision.";

    }

    container Portal {

        container PortalServers {

            description "PortalServers table

                This table supports creating rows.";

            list PortalServer {

                description "Row name";

                key "Name";

                leaf Name {

                    type base:Basetype_String {

                        length "1..32";

                    }

                    description "portal name";

                }

                container IPAddressInfo {

                    description "IPAddressInfo";

                    leaf IPAddress {

                        type base:Basetype_IpAddress;

                        description "IP/IPv6 Address";

                    }

                    leaf VRF {

                        type base:Basetype_VRF {

                            length "1..31";

                        }

                        description "Specifies VRF instance name";

                    }

                    leaf Key {

                        type base:Basetype_String {

                            length "1..64";

                        }

                        description "shared key";

                    }

                }

                leaf Port {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..65534";

                    }

                    description "Port";

                }

                container Detect {

                    description "Servers Detect";

                    leaf TimeOut {

                        type base:Basetype_UnsignedInteger32 {

                            range "10..3600";

                        }

                        description "Detection timeout";

                    }

                    leaf Action {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                        }

                        description "Action";

                    }

                }

                leaf UserSyncTime {

                    type base:Basetype_UnsignedInteger32 {

                        range "60..18000";

                    }

                    description "User synchronization";

                }

            }

        }

        container WebServers {

            description "WebServers table

                This table supports creating rows.";

            list WebServer {

                description "Row name";

                key "Name";

                leaf Name {

                    type base:Basetype_String {

                        length "1..32";

                    }

                    description "portal name";

                }

                leaf Url {

                    type base:Basetype_URLString {

                        length "1..256";

                    }

                    description "URL address";

                }

                container Detect {

                    description "Servers Detect";

                    leaf Interval {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..1200";

                        }

                        description "Interval time";

                    }

                    leaf Retry {

                        type base:Basetype_UnsignedInteger32 {

                            range "1..10";

                        }

                        description "Retry times";

                    }

                    leaf Action {

                        type enumeration {

                            enum 1;

                            enum 2;

                            enum 3;

                        }

                        description "action";

                    }

                }

                leaf VRF {

                    type base:Basetype_VRF {

                        length "1..31";

                    }

                    description "Specifies VRF instance name";

                }

                leaf ServerType {

                    type enumeration {

                        enum 0;

                        enum 1;

                        enum 2;

                        enum 3;

                        enum 4;

                        enum 5;

                    }

                    description "Server Type";

                }

                leaf CaptiveBypass {

                    type enumeration {

                        enum 0;

                        enum 1;

                        enum 2;

                        enum 5;

                        enum 6;

                    }

                    description "Captive Bypass";

                }

                container ServerDetect {

                    description "Server Detect Url";

                    leaf DetectUrl {

                        type base:Basetype_URLString {

                            length "1..256";

                        }

                        description "Server detect url";

                    }

                    leaf DetectType {

                        type enumeration {

                            enum 0;

                            enum 1;

                        }

                        description "Server detect type";

                    }

                }

            }

        }

        container UrlParams {

            description "Portal UrlParams table

                This table supports creating rows.";

            list UrlParam {

                description "Row name";

                key "WebServerName ParamName";

                leaf WebServerName {

                    type base:Basetype_String {

                        length "1..32";

                    }

                    description "portal name";

                }

                leaf ParamName {

                    type base:Basetype_String {

                        length "1..32";

                    }

                    description "URL address";

                }

                leaf ParamType {

                    type enumeration {

                        enum 0;

                        enum 1;

                        enum 2;

                        enum 3;

                    }

                    description "URL address";

                }

                leaf SelfDefValue {

                    type base:Basetype_URLString {

                        length "1..256";

                    }

                    description "URL address";

                }

            }

        }

        container FreeRules {

            description "FreeRules table

                This table supports creating rows.";

            list FreeRule {

                description "Row name";

                key "RuleID";

                leaf RuleID {

                    type base:Basetype_UnsignedInteger32;

                    description "RuleID";

                }

                leaf IsAllotID {

                    type base:Basetype_Boolean;

                    description "Allocation RuleID";

                }

                leaf Description {

                    type base:Basetype_Text {

                        length "1..255";

                    }

                    description "Description for portal free-rule";

                }

                container Layer3 {

                    description "Layer3";

                    leaf SrcIPAddr {

                        type base:Basetype_IpAddress;

                        description "Ip/Ipv6 Address";

                    }

                    leaf SrcIPAddrLen {

                        type base:Basetype_UnsignedInteger32 {

