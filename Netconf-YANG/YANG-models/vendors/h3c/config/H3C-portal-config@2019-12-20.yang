                leaf SrcSsidName {

                    type base:Basetype_String {

                        length "1..32";

                    }

                    description "Source SsidName";

                }

                leaf DstHostName {

                    type base:Basetype_String {

                        length "1..253";

                    }

                    description "Destination HostName";

