                    description "Description .String, case-sensitive.Length: 1 to 200 characters.";

                }

                leaf Frequency {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..604800000";

                    }

                    description "Frequency. Unsigned integer.Value range: 0 to 604800000.Default: 5000.";

                }

                leaf ProbeTimeout {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..4294967295";

                    }

                    description "ProbeTimeout. Unsigned integer Range from 10 to 86400000.for Ftp Range from 10 to 3600000.for others Default: 3000.";

                }

                leaf TTL {

                    type base:Basetype_UnsignedInteger32 {

                        range "1..255";

                    }

                    description "TTL. Unsigned integer.Value range: 1 to 255.Default: 20.";

                }

                leaf ToS {

                    type base:Basetype_UnsignedInteger32 {

                        range "0..255";

                    }

                    description "ToS. Unsigned integer.Value range: 0 to 255.Default: 0.";

