	typedef Basetype_OctectString

	{

		type string;

		description "Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_String

	{

		type string

		{

			pattern "[^?

]*";

		}

		description "String must not contain ?, Tab, and  \r\n characters, but can contain escape characters.

					In xml,the patten is  ^[^?&#x09;&#x0d;&#x0a;]*$

					Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_NameString

	{

		type Basetype_String

		{

			pattern

		"([^?

|>]|[^?

>]{2,2}|[^?

]{3,})";

		}

		description "String must not contain ?, Tab, and  \r\n characters, but can contain escape characters excluding > , >> and |.

					In xml,the patten is  ^([^?&#x09;&#x0d;&#x0a;|&gt;]|[^?&#x09;&#x0d;&#x0a;&gt;]{2,2}|[^?&#x09;&#x0d;&#x0a;]{3,})$

					Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_Text

	{

		type Basetype_OctectString

		{

			pattern "([^?

 ][^?

]*[^?

 ]|[^?

 ])";

		}

		description "String must not contain ?, Tab, \r\n and escape characters, and must not begin or end with space character.

					In xml,the patten is  ^([^?&#x09;&#x0d;&#x0a; ][^?&#x09;&#x0d;&#x0a;]*[^?&#x09;&#x0d;&#x0a; ]|[^?&#x09;&#x0d;&#x0a; ])$

					Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_EncryptString

	{

		type Basetype_OctectString

		{

			pattern "[^?]*";

		}

		description "Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_URLString

	{

		type Basetype_OctectString

		{

			pattern "[^?

][^

]*";

		}

		description "String must not contain Tab, \r\n and escape characters, and must not begin with ? character.

					In xml,the patten is  ^[^?&#x09;&#x0d;&#x0a;][^&#x09;&#x0d;&#x0a;]*$

					 Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_Boolean

