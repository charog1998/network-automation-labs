		type boolean;

		description "Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_DateTime

	{

		type string

		{

			pattern "[0-9]{4}-[0-1][0-9]-[0-3][0-9]T[0-2][0-9]:[0-5][0-9]:[0-5][0-9](\.[0-9]{1,20})?(Z|[+|-][0-9]{2}:[0-9]{2})?";

		}

		description "Date And Time.

					 Additional xml attributes for this type: match, regExp, incremental";

	}

