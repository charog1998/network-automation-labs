	namespace "http://www.h3c.com/netconf/basetype:1.0";

	prefix comware-basetype;

	organization "H3C";

	contact "New H3C Technologies Co., Ltd.";

	description "";

	revision 2019-07-30

	{

		description "Synchronize the modification of the schema file to the YANG file.";

	}

	revision 2014-10-12

	{

		description "Initial revision";

