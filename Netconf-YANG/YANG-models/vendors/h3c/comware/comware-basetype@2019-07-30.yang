	typedef Basetype_MacAddress

	{

		type string

		{

			pattern "(([a-fA-F0-9]{1,2}-){5})([a-fA-F0-9]{1,2})";

		}

		description "MAC address.

					 Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_Ipv4Address

	{

	    type string

		{

			pattern "((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|0?[1-9][0-9]|0{0,2}[0-9])\.){3}(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|0?[1-9][0-9]|0{0,2}[0-9])";

		}

		description "IPv4 Address.

					 Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_Ipv6Address

	{

		type string;

		description "IPv6 address, with validity check inside.

					 Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_IpAddress

	{

		type union

		{

		    type Basetype_Ipv4Address;

			type Basetype_Ipv6Address;

		}

		description "IPv4 or IPv6 address.

					 Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_InterfaceName

	{

		type string

		{

			pattern "([a-zA-Z][-a-zA-Z]{1,19}|[a-df-su-zA-DF-SU-Z][-a-zA-Z]{0,19}|((E1|T1|E3|T3) ))(([1-9][0-9]{0,2}|[0-9])/)?(([1-9][0-9]{0,2}|[0-9])/)?(([1-9][0-9]{0,2}|[0-9])/)?([1-9][0-9]{0,4}|[0-9])(:([1-9][0-9]{0,3}|[0-9]))?(\.([1-9][0-9]{0,3}|[0-9]))?";

		}

		description "Interface name. String length constraints must be in range(2..47).

					 Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_ObjectIdentifier

	{

		type string;

		description "OID, with validity check inside.

					Additional xml attributes for this type: match, regExp, incremental";

	}

