    typedef Basetype_ACLName

	{

		type string

		{

			pattern "([a-zA-Z]{1}.*)";

			length "1..63";

		}

		description "ACL name:length :1-63,case-insensitive,must start with a-z or A-Z

					 Additional xml attributes for this type: match, regExp, incremental";

	}

