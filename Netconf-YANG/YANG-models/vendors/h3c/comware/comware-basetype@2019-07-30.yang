		type string

		{

			pattern "[0-9][0-9][0-9][0-9]-[0-1][0-9]-[0-3][0-9](Z|[+|-][0-2][0-9]:[0-5][0-9])?";

		}

		description "Additional xml attributes for this type: match, regExp, incremental";

	}

	typedef Basetype_Time

